@model List<Message>
@{
}
<h1>
    Stories about Rivers Cuomo and his band Weezer.
</h1>

@if (@Model != null)
{
   
    @foreach (var message in Model)
    {
        <p>
            From: @message.Username <br>
            Title: @message.Title
            Topic: @message.Topic
            Year the Story Takes Place: @message.YearDate
            Story: @message.StoryBody <br>
            Date: @message.Date
        </p>
    }
}

